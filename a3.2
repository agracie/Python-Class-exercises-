# This code prompts user to enter a decimal grade then assigns a letter grade
score = raw_input("Enter Score:")
s = float(score)
if s > 1:
    print "Please use number between 0.0 and 1"
elif s < 0:
    print "Please use number between 0.0 and 1"
elif s >= 0.9:
    print "A"
elif s >= 0.8:
    print "B"
elif s >= 0.7:
    print "C"
elif s >= 0.6:
    print "D"
elif s < 0.6:
    print "F"



